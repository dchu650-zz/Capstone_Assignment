<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aidan.contactmanager" >

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.read_external_storage"/>
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name= "android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>






    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        android:debuggable="true"


        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DisplayItem"
            android:label="@string/title_activity_display_item" >
        </activity>
        <activity android:name=".MyProfile"> </activity>
        <activity android:name=".MyItems"> </activity>
        <activity android:name=".Interests"> </activity>
        <activity android:name=".EditItem"></activity>
    </application>

</manifest>
